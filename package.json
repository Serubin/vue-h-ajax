{
  "name": "vue-h-ajax",
  "version": "0.1.1",
  "description": "Lightweight ajax module for Vue.js 2",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "keywords": [
    "vue",
    "http",
    "ajax",
    "xhr"
  ],
  "scripts": {
    "prebuild": "eslint index.js",
    "build": "babel index.js --out-file dist/index.js --source-maps inline",
    "prepare": "npm run build",
    "lint": "eslint index.js",
    "test": "jest"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text-summary",
      "html"
    ]
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "author": "Alexander Zonov",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-jest": "^18.0.0",
    "babel-preset-es2015": "^6.22.0",
    "eslint": "^3.15.0",
    "jest": "^21.2.1",
    "vue": "^2.1.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zont/vue-h-ajax"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
